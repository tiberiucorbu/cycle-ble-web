{
  "name": "webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:ts": "npx tsc -p .",
    "build:js": "npx gulp default",
    "build:jses5": "npx gulp es5",
    "clean": "npx gulp clean",
    "build": "npx gulp",
    "start": "webpack-dev-server --mode=development"
  },
  "author": {
    "name": "Tiberiu  Corbu",
    "email": "tiberiu.corbu@gmail.com"
  },
  "main": "dist/bundle.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/polyfill": "7.6.0",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-typescript": "7.6.0",
    "babel-loader": "8.0.6",
    "core-js": "3.2.1",
    "del": "5.1.0",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "html-webpack-plugin": "3.2.0",
    "ts-loader": "6.1.2",
    "typescript": "3.6.3",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1",
    "webpack-stream": "5.2.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "2.3.0",
    "lit-element": "^2.2.1"
  }
}
