{
  "name": "webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc -p .",
    "build:js": "gulp default",
    "build:jses5": "gulp es5",
    "clean": "gulp clean",
    "build": "gulp",
    "start": "webpack-dev-server --mode=development"
  },
  "author": {
    "name": "Tiberiu  Corbu",
    "email": "tiberiu.corbu@gmail.com"
  },
  "main": "dist/bundle.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/mathjs": "6.0.1",
    "@types/web-bluetooth": "0.0.4",
    "babel-loader": "8.0.6",
    "core-js": "3.2.1",
    "del": "5.0.0",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "html-webpack-plugin": "3.2.0",
    "math.js": "1.1.43",
    "raw-loader": "^3.1.0",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.8.0",
    "webpack-stream": "5.2.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "dependencies": {
    "@types/leaflet": "^1.4.3",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "leaflet": "^1.4.0",
    "lit-element": "^2.0.1"
  }
}
