{
  "name": "webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc -p .",
    "build:js": "gulp default",
    "build:jses5": "gulp es5",
    "clean": "gulp clean",
    "build": "gulp",
    "start": "webpack-dev-server --mode=development"
  },
  "author": {
    "name": "Tiberiu  Corbu",
    "email": "tiberiu.corbu@gmail.com"
  },
  "main": "dist/bundle.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/polyfill": "7.4.0",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-typescript": "7.3.3",
    "babel-loader": "8.0.5",
    "core-js": "3.0.0",
    "del": "4.0.0",
    "fork-ts-checker-webpack-plugin": "1.0.0",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "html-webpack-plugin": "3.2.0",
    "ts-loader": "5.3.3",
    "typescript": "3.2.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-stream": "5.2.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "2.2.7",
    "lit-element": "^2.0.1"
  }
}
